			{% for svg in svgEmbarcado %}
			<symbol id="{{ svg.id }}" viewBox="{{ svg.viewBox }}">
				<path
					d="{{ svg.dados }}" />
			</symbol>
			{% endfor %}
		</defs>
	</svg>

	<!-- Cabeçalho -------------------------------------------------------------->
	<header role="banner" id="cabecalho">
		{% include "partials/cabecalho.njk" %}
	</header>

	<!-- Conteúdo principal ----------------------------------------------------->
	<main tabindex="-1" id="conteudo-principal">
		{{ content | safe }}
		<section class="base-cta">
			<a href="https://wa.me/{{ cabecalho.whatsapp }}/">Agendar consulta</a>
		</section>
	</main>

<!-- Rodapé ------------------------------------------------------------------->
<footer role="contentInfo" class="site-foot">
	{% include "partials/rodape.njk" %}
</footer>

</body>

</html>